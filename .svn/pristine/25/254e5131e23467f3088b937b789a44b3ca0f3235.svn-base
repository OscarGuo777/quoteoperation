<script>
    $(document).ready(function(){
        // 初始化内容
        $("#main_cnt").hide();
    });
    function loadPage(url,obj) {
        $("#main_cnt").show();
        var atxt = $(obj).text();
        $("#cur_page_nm").html(atxt);
        $("h4.page-title").html(atxt);
        $("#content").load(url);
    }
