<script>
    $(document).ready(function(){
        // 初始化内容
        $("#main_cnt").hide();
    });
    function loadPage(url,obj) {
        $("#main_cnt").show();
        var atxt = $(obj).text();
        $("#cur_page_nm").html(atxt);//配置表单上的导航链接名
        $("h4.page-title").html(atxt);//配置表单功能名
        $("#content").load(url);
    }
</script>
