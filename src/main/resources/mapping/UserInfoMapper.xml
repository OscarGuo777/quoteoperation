  <select id="queryAllUser" resultMap="UserDepartBoMap">
    SELECT u.LGC_SN,u.USR_ACNT,u.USR_NM,u.USR_MOBI,u.STATUS,u.CRT_DT,u.CRT_USR_ID,d.DEPT_NM,u.USR_MAIL,ui.USR_ACNT AS CURRENT_USR_ACNT
    FROM qo_user_info u
    INNER JOIN qo_user_dept ud ON u.LGC_SN = ud.USR_SN
    INNER JOIN qo_dept_info d ON ud.DEPT_SN = d.LGC_SN
    INNER JOIN qo_user_info ui ON ui.LGC_SN = u.CRT_USR_ID
  </select>

  <select id="selectUserByKeyword" parameterType="map" resultMap="UserDepartBoMap">
    SELECT u.LGC_SN,u.USR_ACNT,u.USR_NM,u.USR_MOBI,u.STATUS,u.CRT_DT,u.CRT_USR_ID,d.DEPT_NM,u.USR_MAIL,u.USR_MAIL,ui.USR_ACNT AS CURRENT_USR_ACNT
    FROM qo_user_info u
    INNER JOIN qo_user_dept ud ON u.LGC_SN = ud.USR_SN
    INNER JOIN qo_dept_info d ON ud.DEPT_SN = d.LGC_SN
    INNER JOIN qo_user_info ui ON ui.LGC_SN = u.CRT_USR_ID
    <where>
      <if test=" status != null and status != ''">
        AND u.status = #{status}
      </if>
      <if test="keyword != null and keyword != ''">
        AND (
        u.USR_ACNT  LIKE  CONCAT ('%',TRIM(#{keyword}),'%')
        OR  u.USR_MOBI LIKE  CONCAT ('%',TRIM(#{keyword}),'%')
        OR  u.USR_NM LIKE  CONCAT ('%',TRIM(#{keyword}),'%')
        )
      </if>
    </where>
  </select>

